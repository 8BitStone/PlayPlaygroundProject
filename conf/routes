GET     /                 controllers.HomeController.index(request: Request)
GET     /te1              controllers.TemplateEngineV1Controller.index(request: Request)
GET     /te1/list         controllers.TemplateEngineV1Controller.articleList(request: Request)
GET     /te1/form         controllers.TemplateEngineV1Controller.form(request: Request)
POST    /te1/form         controllers.TemplateEngineV1Controller.formSubmit(request: Request)

GET     /te2              controllers.TemplateEngineV2Controller.index(request: Request)
GET     /te2/list         controllers.TemplateEngineV2Controller.articleList(request: Request)
GET     /te2/form         controllers.TemplateEngineV2Controller.form(request: Request)

GET     /te2/api/header   controllers.TemplateEngineV2Controller.headerData(request: Request)
GET     /te2/api/footer   controllers.TemplateEngineV2Controller.footerData(request: Request)
GET     /te2/api/list     controllers.TemplateEngineV2Controller.listData(request: Request)
POST    /te2/api/form     controllers.TemplateEngineV2Controller.formSubmit(request: Request)

GET     /assets/*file     controllers.Assets.versioned(path="/public", file: Asset)
